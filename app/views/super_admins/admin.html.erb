<h1>Admin</h1>

<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <%= form_for :session, url: admin_path do |f| %>
    
        <%= f.label :username %>
        <%= f.text_field :username %>
    
        <%= f.label :password %>
        <%= f.password_field :password %>
        <%= f.submit 'Log In', class: "btn btn-primary" %>
        <% end %>
    </div>
</div>