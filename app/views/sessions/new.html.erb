<% provide(:title, "Log in") %>
<%= form_for :session, url: login_path do |f| %>
    
    <%= f.label :email %>
    <%= f.email_field :email %>
    
    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.submit 'Log In' %>
<% end %>
<p>New User? <%= link_to 'Sign up', new_user_path %></p>
    