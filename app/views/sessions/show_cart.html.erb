Cart:&nbsp;<%= session[:cart] %> <br>
<% if session[:cart] %>
<% session[:cart].each do |item_id, quantity|%>
    <% item = Item.find_by(id: item_id) %>
    <% if item %>
    <%= item.name %>
    <%= item.price %>
    <%= quantity %>
    <%= button_to "Remove from cart", delete_from_cart_path(item) %>
<% end %>
<% end %>
<% end %>
