<h1>Item id: <%= @item.id %></h1>
    <br>
    <% if @item.user %>
    Owner: <%= @item.user.first_name %> <%= @item.user.last_name %>
    <% end %>
    
    Seller: <%= @item.user.name %>
    <br>
    
    Item name: <%= @item.name %>
    <br>
    
    Price: $<%= @item.price %>
    <br>
    
    Quantity: <%= @item.quantity %>
    <br>
    
    Description: <%= @item.desc %>